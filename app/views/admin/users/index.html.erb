<center>
<h1>All Users</h1>
</center>
  <% @users.each do |user| %>
    <section id= "user-<%= user.id %>">
      <p>User Name: <%=link_to user.name, "/admin/users/#{user.id}" %></p>
      <p>User Registration Date: <%= user.created_at %></p>
      <p>User Role: <%= user.role %></p>
    </section>
  <% end %><br>
